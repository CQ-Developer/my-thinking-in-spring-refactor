<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="user"
          class="huhu.io.thinking.in.spring.dependency.model.User">
        <property name="id" value="1"/>
        <property name="name" value="huhu"/>
    </bean>

    <bean id="userObjectFactory"
          class="org.springframework.beans.factory.config.ObjectFactoryCreatingFactoryBean">
        <property name="targetBeanName" value="user"/>
    </bean>

    <bean id="superUser"
          class="huhu.io.thinking.in.spring.dependency.model.SuperUser"
          parent="user">
        <property name="address" value="nanjing"/>
    </bean>

</beans>
